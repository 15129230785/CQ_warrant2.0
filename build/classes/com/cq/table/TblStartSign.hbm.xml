<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cq.table.TblStartSign" table="tbl_startsign">
		<id name="kid" type="java.lang.Integer">
			<column name="KID" />
			<generator class="identity" />
		</id>
		<property name="wid" type="string">
			<column name="WID" length="60" not-null="true" />
		</property>
		<property name="name" type="string">
            <column name="Name" length="60" />
        </property>
		<property name="date" type="date">
			<column name="Date" length="10" />
		</property>
		<property name="checks" type="string">
			<column name="Checks" length="1024" />
		</property>
	</class>
</hibernate-mapping>
