<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cq.table.TblCfgTaskgroup" table="tbl_cfg_taskgroup">
        <id name="kid" type="java.lang.Integer">
            <column name="KID"/>
            <generator class="identity" />
        </id>
        <property name="taskid" type="string">
            <column name="TASKID" length="40" not-null="true" />
        </property>
        <property name="seq" type="java.lang.Integer">
            <column name="SEQ" length="40" not-null="true" />
        </property>
        <property name="groupid" type="string">
            <column name="GROUPID" length="40" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
