package com.cq.table;

import java.io.Serializable;
import java.util.Date;

public class TblFieldsurvey implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private int kid;
	private String wid;
	private String name;
	private Date startDate;
	private Date endDate;
	private String product;
	private String equipment;
	private String manage;
	private String staff;
	private String workplace;
	private String counterGuarantee;
	private String loausanalysis;

	public TblFieldsurvey() {
	}

	public TblFieldsurvey(String wid, String name, Date startDate, Date endDate) {
		this.wid = wid;
		this.name = name;
		this.startDate = startDate;
		this.endDate = endDate;
	}

	public TblFieldsurvey(String wid, String name, Date startDate,
			Date endDate, String product, String equipment, String manage,
			String staff, String workplace, String rawmaterial, String stock,
			String ambience, String overall,String loausanalysis) {
		this.wid = wid;
		this.name = name;
		this.startDate = startDate;
		this.endDate = endDate;
		this.product = product;
		this.equipment = equipment;
		this.manage = manage;
		this.staff = staff;
		this.workplace = workplace;
	}


	public int getKid() {
		return kid;
	}

	public void setKid(int kid) {
		this.kid = kid;
	}

	public String getWid() {
		return this.wid;
	}

	public void setWid(String wid) {
		this.wid = wid;
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Date getStartDate() {
		return this.startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return this.endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public String getProduct() {
		return this.product;
	}

	public void setProduct(String product) {
		this.product = product;
	}

	public String getEquipment() {
		return this.equipment;
	}

	public void setEquipment(String equipment) {
		this.equipment = equipment;
	}

	public String getManage() {
		return this.manage;
	}

	public void setManage(String manage) {
		this.manage = manage;
	}

	public String getStaff() {
		return this.staff;
	}

	public void setStaff(String staff) {
		this.staff = staff;
	}

	public String getWorkplace() {
		return this.workplace;
	}

	public void setWorkplace(String workplace) {
		this.workplace = workplace;
	}

	public String getCounterGuarantee() {
		return counterGuarantee;
	}

	public void setCounterGuarantee(String counterGuarantee) {
		this.counterGuarantee = counterGuarantee;
	}
	public String getLoausanalysis() {
		return loausanalysis;
	}

	public void setLoausanalysis(String loausanalysis) {
		this.loausanalysis = loausanalysis;
	}


}
