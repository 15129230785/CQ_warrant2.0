<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cq.table.TblRegshareholder" table="tbl_regshareholder">
        <id name="kid" type="java.lang.Integer">
            <column name="KID"/>
            <generator class="identity" />
        </id>
        <property name="rid" type="string">
            <column name="RID" length="20" not-null="true" />
        </property>
        <property name="type" type="java.lang.Character">
            <column name="Type" length="1" not-null="true" />
        </property>
        <property name="name" type="string">
            <column name="Name" length="60" not-null="true" />
        </property>
        <property name="sid" type="string">
            <column name="SID" length="18" not-null="true" />
        </property>
        <property name="share" type="java.lang.Double">
            <column name="Share" precision="22" scale="0" />
        </property>
    </class>
</hibernate-mapping>
