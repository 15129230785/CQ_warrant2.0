package com.cq.table;

import java.io.Serializable;
import java.util.Date;

public class TblProblemtrack implements Serializable {
	private static final long serialVersionUID = 1L;
	
	private int kid;
	private String wid;
	private String state;
	private String name;
	private Date startDate;
	private Date endDate;
	private String person;
	private Character type;
	private String description;
	private String explains;
	private String result;
	private int times;
	
	public TblProblemtrack() {
	}

	public TblProblemtrack(int kid,String wid, String state, String name, Date startDate,
			String person, Character type) {
		this.kid = kid;
		this.wid = wid;
		this.state = state;
		this.name = name;
		this.startDate = startDate;
		this.person = person;
		this.type = type;
	}

	public TblProblemtrack(int kid,String wid, String state, String name, Date startDate,
			Date endDate, String person, Character type, String description,
			String explains, int times) {
		this.kid = kid;
		this.wid = wid;
		this.state = state;
		this.name = name;
		this.startDate = startDate;
		this.endDate = endDate;
		this.person = person;
		this.type = type;
		this.description = description;
		this.explains = explains;
		this.times = times;
	}

	public int getKid() {
		return kid;
	}

	public void setKid(int kid) {
		this.kid = kid;
	}

	public String getWid() {
		return this.wid;
	}

	public void setWid(String wid) {
		this.wid = wid;
	}

	public String getState() {
		return state;
	}

	public void setState(String state) {
		this.state = state;
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Date getStartDate() {
		return this.startDate;
	}

	public void setStartDate(Date startDate) {
		this.startDate = startDate;
	}

	public Date getEndDate() {
		return this.endDate;
	}

	public void setEndDate(Date endDate) {
		this.endDate = endDate;
	}

	public String getPerson() {
		return this.person;
	}

	public void setPerson(String person) {
		this.person = person;
	}

	public Character getType() {
		return this.type;
	}

	public void setType(Character type) {
		this.type = type;
	}

	public String getDescription() {
		return this.description;
	}

	public void setDescription(String description) {
		this.description = description;
	}

	public String getExplains() {
		return this.explains;
	}

	public void setExplains(String explains) {
		this.explains = explains;
	}

	public String getResult() {
		return result;
	}

	public void setResult(String result) {
		this.result = result;
	}

	public int getTimes() {
		return times;
	}

	public void setTimes(int times) {
		this.times = times;
	}
}
