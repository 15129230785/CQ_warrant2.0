package com.cq.table;

import java.io.Serializable;

public class TblCfgCalculateindex implements Serializable {
	private static final long serialVersionUID = 7184668366825427977L;
	
	private int kid;
	private String name;
	private String cname;
	private double floor;
	private double ceil;
	private double indexvalue;
	private double ratio;

	public TblCfgCalculateindex() {
	}

	public TblCfgCalculateindex(int kid, String name, String cname, double floor,
			double ceil, double indexvalue, double ratio) {
		this.kid = kid;
		this.name = name;
		this.setCname(cname);
		this.floor = floor;
		this.ceil = ceil;
		this.indexvalue = indexvalue;
		this.ratio = ratio;
	}
	
	public int getKid() {
		return kid;
	}

	public void setKid(int kid) {
		this.kid = kid;
	}

	public String getName() {
		return this.name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public double getFloor() {
		return this.floor;
	}

	public void setFloor(double floor) {
		this.floor = floor;
	}

	public double getCeil() {
		return this.ceil;
	}

	public void setCeil(double ceil) {
		this.ceil = ceil;
	}

	public double getRatio() {
		return this.ratio;
	}

	public void setRatio(double ratio) {
		this.ratio = ratio;
	}

	public String getCname() {
		return cname;
	}

	public void setCname(String cname) {
		this.cname = cname;
	}

	public double getIndexvalue() {
		return indexvalue;
	}

	public void setIndexvalue(double indexvalue) {
		this.indexvalue = indexvalue;
	}
}
