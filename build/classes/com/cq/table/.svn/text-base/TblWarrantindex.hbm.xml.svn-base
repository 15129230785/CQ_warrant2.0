<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.cq.table.TblWarrantindex" table="tbl_warrantindex">
        <id name="kid" type="java.lang.Integer">
            <column name="KID"/>
            <generator class="identity" />
        </id>
        <property name="wid" type="string">
            <column name="WID" length="60" not-null="true" />
        </property>
        <property name="id" type="string">
            <column name="ID" length="60" />
        </property>
        <property name="type" type="java.lang.Character">
            <column name="Type" length="1" />
        </property>
        <property name="name" type="string">
            <column name="Name" length="60" />
        </property>
        <property name="times" type="integer">
            <column name="Times" length="1" default="0" />
        </property>
    </class>
</hibernate-mapping>
