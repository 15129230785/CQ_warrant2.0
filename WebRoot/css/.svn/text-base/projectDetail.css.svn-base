body {
	font: normal 12px "宋体";
	background: url(../tabs/images/bg.gif) repeat-x 0 5px;
	color: #474747;
}
table{padding:10px 0 0;}
#prodetailsDiv table
 {
	border: 1px solid #B1CDE3;
	border-collapse: collapse;
	width: 98%;
	margin:10px;
}

#TblAntiwarrantdiv table
 {
	border: 1px solid #B1CDE3;
	border-collapse: collapse;
	width: 98%;
/* 	margin:10px; */
}

#prodetailsDiv table th, #TblAntiwarrantdiv table th
{
	font: bold 12px/30px "宋体";
	color: #797268;
	background: #D8E0E0;
	border: 1px solid #476074;
	border-collapse: collapse;
	line-height:150%;
	text-align:center;
}

#prodetailsDiv table td
{
	border: 1px solid #B1CDE3;
	font: normal 12px "宋体";
	padding: 3px 0;
	color: #4f6b72;
	text-align: left;
}

#TblAntiwarrantdiv table td
{
	border: 1px solid #B1CDE3;
	background: #fff;
	font: normal 12px "宋体";
	padding: 3px 0;
	color: #4f6b72;
	text-align: center;
}

a {
	color: #4f6b72;
 	line-height:150%;
	text-decoration: none;
}

a:hover {
	color: #ff3300;
	text-decoration: underline;
}