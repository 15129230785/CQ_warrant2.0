<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.cq.table.TblCfgAnalysisindex" table="tbl_cfg_analysisindex">
		<id name="kid" type="java.lang.Integer">
			<column name="KID" />
			<generator class="identity" />
		</id>
		<property name="name" type="string">
			<column length="255" name="name" not-null="true" />
		</property>
		<property name="cname" type="string">
			<column length="255" name="cname" not-null="true" />
		</property>
		<property name="ratio" type="java.lang.Double">
			<column name="ratio" not-null="true" />
		</property>
	</class>
</hibernate-mapping>
